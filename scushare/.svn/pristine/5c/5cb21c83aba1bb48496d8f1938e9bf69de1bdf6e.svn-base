    	 .bor{
        	border-bottom:1px solid #EEEEEE;
        	width: 100%;
        	height: 141px;
        	position: relative;
        	float: left;
        	margin-top:20px;

        }
        .bor h3{
        	width: 100%;
        	position: relative;
        	float: left;
        	margin-bottom: 2px;
        }
        .bor .like-share{
        	position: relative;
        	float: right;
        	list-style: none;
        	cursor: pointer;
        	font-size: 15px;
        	margin-top: 0px;
        	margin-right: 30px;
        	font-weight: 400;
        }
        .bor .like-share li{
        	position: relative;
        	float: left;
        	margin-left: 35px;
        }
        .bor img{
        	float: left;
        	width: 18px;
        	height: 18px;
        	margin-top: 3px;
        	margin-right: 8px;
        }
        .bor .title{
        	color: #2c63b3;
		    float: left;
		    margin-right: 5px;
		    font-size: 16px;
		    max-width: 740px;
		    text-decoration: underline;
		    overflow: hidden;
		    text-overflow: ellipsis;
		    white-space: nowrap;
        }

        .bor .material{
        	position: relative;
        	float: left;
        }
        .bor .material a{
        	background: url("../image/cover.png") center top no-repeat scroll;
        	width: 100px;
    		height: 57px;
    		display: block;
    		position: relative;
    		float: left;
        }
        .bor .info{
        	position: relative;
        	float: left;
        	width: 623px;
        	height: 64px;
        }
        .bor .desc{
        	position: relative;
        	float: left;
        	width:623px;
        	height: 40px;
        	font-size: 15px;
        	overflow: hidden;
        }
        .bor .intro{
        	position: relative;
        	float: left;
        	width: 623px;
        	height:24px;
        	margin-top: 3px;
        	font-size: 10px;
        }
        .bor .intro span{
        	position: relative;
        	float: left;
        	color: #000000; 
        	font-weight: 400;
        }
        .margin{
        	margin-left: 20px;
        	margin-right: 20px;
        }
        .page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 34px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
        #change-page{
        	width: 100%;
        	height: 50px;
        	position: relative;
        	float: left;
        	margin-top: 30px;
        	margin-bottom: 50px;
        }
        #change-page ul{
        	float: left;
        	margin-left: 40px;
        }
        #last-page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 60px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
        #change-page .next-page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 60px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
        #mission{
        	position: fixed;
        	top:18%;
        	left: 80%;
        }
        #mission button{
            width: 75%;
            height: 40px;
            font-size: 20px;
            background-color:#00B0FF;
            color: #ffffff;
            border:0px;
            cursor: pointer;
        }
        #mission p{
            margin-top: 30px;
            font-size: 15px;
        }
        #mission p span{
            color: green;
        }
        #deep-blue-span{
        	color:blue;
        }