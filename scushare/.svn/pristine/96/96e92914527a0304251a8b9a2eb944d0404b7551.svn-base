        body{
    		margin: 0px;
    		padding: 0px;
    	}
    	.black-span{
        	color: #000000;
        }
    	.red-span{
        	color: red;
        }
        .white-span{
        	color: #ffffff;
        }
        .bor{
        	border-bottom:1px solid #EEEEEE;
        	width: 100%;
        	height: 141px;
        	position: relative;
        	float: left;
        	margin-top:20px;

        }
        .bor h3{
        	width: 100%;
        	position: relative;
        	float: left;
        	margin-bottom: 2px;
        }
        .bor .like-share{
        	position: relative;
        	float: right;
        	list-style: none;
        	cursor: pointer;
        	font-size: 15px;
        	margin-top: 0px;
        	margin-right: 30px;
        	font-weight: 400;
        }
        .bor .like-share li{
        	position: relative;
        	float: left;
        	margin-left: 35px;
        }
        .bor img{
        	float: left;
        	width: 18px;
        	height: 18px;
        	margin-top: 3px;
        	margin-right: 8px;
        }
        .bor .title{
        	color: #2c63b3;
		    float: left;
		    margin-right: 5px;
		    font-size: 16px;
		    max-width: 740px;
		    text-decoration: underline;
		    overflow: hidden;
		    text-overflow: ellipsis;
		    white-space: nowrap;
        }

        .bor .material{
        	position: relative;
        	float: left;
        }
        .bor .material a{
        	background: url("../image/cover.png") center top no-repeat scroll;
        	width: 100px;
    		height: 57px;
    		display: block;
    		position: relative;
    		float: left;
        }
        .bor .info{
        	position: relative;
        	float: left;
        	width: 623px;
        	height: 64px;
        }
        .bor .desc{
        	position: relative;
        	float: left;
        	width:623px;
        	height: 40px;
        	font-size: 15px;
        	overflow: hidden;
        }
        .bor .intro{
        	position: relative;
        	float: left;
        	width: 623px;
        	height:24px;
        	margin-top: 3px;
        	font-size: 10px;
        }
        .bor .intro span{
        	position: relative;
        	float: left;
        	color: #000000; 
        	font-weight: 400;
        }
        .margin{
        	margin-left: 20px;
        	margin-right: 20px;
        }
        .page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 34px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
        #last-page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 60px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
    	#navbar{
    		margin:0px;
    		padding: 0px;
    		width: 100%;
    		height: 50px;
            background-color: rgba(0,0,0,0.7);
    	}
    	#nav-left{
    		width: 10%;
    		left: 100px;
    		position: relative;
    		float: left;

    	}
    	#catalog{
    		width: 30%;
    		position: relative;
    		left: 20px;
    		float: left;
    	}
    	#docu{
    		height: 34px;
    	}
    	#catalog li{
    		position: relative;
    		float: left;
    		margin-left: 60px;
    		list-style: none;
    	}
        a{
            text-decoration: none;
        }
    	#user{
    		position: relative;
    		float: left;
    		top: 30%;
    		left: 180px;
    	}
        #login_image{
            position: relative;
    		float: left;
    		top: 25%;
    		left: 180px;
        }
        #login_image img{
            width:30px;
            height:30px;
        }
    	#form{
    		position: relative;
    		float: left;
    		left: 190px;
    		top:25%;
    	}
    	#input-tag{
    		width: 	300px;
    		height: 30px;
    		size:35px;
    		float: left;
    	}
    	#submit{
    		height: 30px;
    	}
    	#input{
    		position: relative;
    		float: left;
    		border-radius: 3px;
    		border:1px solid #000000;
    		
    	}
    	#form button{
    		width: 100px;
    		height: 30px;
    		position: relative;
    		float:left;
    		left: 15px;
    		border:0px;
    		background:#00B0FF;
    		color: #ffffff;
            cursor: pointer;
    	}

    	#down-list{
    		position: fixed;
            top: 50px;
    		height: 200px;
    		width: 500px;
    		background-color:#000000;
    		padding: 0px;
    		display: none;
    	}
    	#down-list li{
    		position: relative;
    		float: left;
    		margin-left: 10px;
    		list-style: none;
    	}
    	#down-list span{
    		font-size: 5px;
    	}
    	#back-img{
            width: 48px;
            height: 48px;
            position: fixed;
            right: 20px;
            bottom: 100px;
        }
        #back-text{
            width: 40px;
            height: 40px;
            position: fixed;
            right: 20px;
            bottom: 100px;
            display: none;
        }
        #back-text span{
            color:black;
        }
        #content{
        	width: 80%;
        	position: relative;
        	left: 13%;
        	top:20px	;
        	height: 1000px;
        }
        #main-content{
        	position: relative;
        	float: left;
        	width: 70%;
        	height: 1000px;
        }
        #catalogue{
        	position: relative;
        	float: left;
        	width: 100%;
        	height: 40px;
        	background: #F8F8F8;
        }
        #catalogue a{
        	position: relative;
        	float: left;
        	margin-left: 20px;
        	top: 25%;	
        }
        #search-result{
        	position: relative;
        	float: right;
        	right: 5%;
        	top: 25%;
        }	
       	#select-format{
       		position: relative;
        	float: left;
        	top:15px;
        	height: 50px;
        	width: 100%;
        	border-bottom:1px dashed #EEEEEE;
       	}
        #filter input{
        	margin-top: 5px;
        	width: 16px;
        	height: 16px;
        }
        #filter span{
        	font-size: 15px;
        	color: black;
        	position: relative;
        }
        #help{
        	width: 100%;
        	height: 20px;
        }
        #help{
        	margin-top: 30px;
        	position: relative;
        	float: left;
        	background: #EEEEEE;
        	height: 50px;
        }
        #help span{
        	position: relative;
        	float: left;
        	top:25%;
        	left: 2%;
        	font-size: 14px;
        	font-weight: 200;
        	margin-bottom: 40px;
        }
        #help a{
        	border: 1px solid #00a1e9;
		    color: #00a1e9;
		    float: right;
		    height: 28px;
		    line-height: 28px;
		    margin: 10px 15px;
		    padding: 0 17px;
		    border-radius: 3px;
		    top:25%;
		    cursor: pointer;
        }
        #change-page{
        	width: 100%;
        	height: 50px;
        	position: relative;
        	float: left;
        	margin-top: 30px;
        	margin-bottom: 50px;
        }
        #change-page ul{
        	float: left;
        	margin-left: 40px;
        	padding:0px;
        }
        #change-page .next-page{
        	display: block;
        	border: solid 1px #F8F8F8;
        	list-style: none;
        	float: left;
        	margin-left: 10px;
        	width: 60px;
        	height: 34px;
        	font-size: 10px;
        	text-align: center;
        	line-height:33px;
        	cursor: pointer;
        }
     	#recommend{
        	position: relative;
        	float: left;
        	width: 30%;
        	height: 1000px;
        	left: 1%;
        }
        #mission{
            float: left;
            width: 100%;
            height: 150px;
            border-bottom:1px dashed #EEEEEE;
        }
        #mission button{
            width: 69%;
            height: 40px;
            font-size: 20px;
            background-color:#00B0FF;
            color: #ffffff;
            border:0px;
            cursor: pointer;
        }
        #mission p{
            margin-top: 30px;
            font-size: 15px;
        }
        #mission p span{
            color: green;
        }
        #recommend-result{
            float: left;
            width: 100%;
            height: 400px;
            margin-top: 15px;
        }
        #recommend-result p{
            margin-left: 5px;
            font-size: 20px;
            width: 100%;
        }
        .recommend-file{
            float: left;
            width: 300px;
            height: 50px;
            background: red;
        }
        #file-info{
            float: left;
            width: 100%;
            height: 50px;
            margin-left: 0px;
            padding-left: 3px;
        }
        #file-info img:not(.star){
            float: left;
            width: 18px;
            height: 18px;
            margin-top: 3px;
            margin-right: 3px;
        }
        #file-info li{
            float: left;
            list-style: none;
        }
        #page-num{
            margin-left: 10px;
        }
        .file{
            margin-top: 15px;
        }